    "primary": "#338e4a",
    "light": "#e1efed",
    "dark": "#15803D"
  },
  "fonts": {
    "family": "area",
    "weight": 700,
    "source": "https://mintlify-fonts.s3.us-east-1.amazonaws.com/AreaNormal-Regular.woff",
